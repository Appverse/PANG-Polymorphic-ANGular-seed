		"postinstall": "git checkout tsconfig.json",
		"typings": "typings install && cd server && typings install",
		"server": "ts-node app/main.server.ts",
		"server:watch": "nodemon --watch 'app/*.ts'  --exec 'ts-node' app/main.server.ts",
